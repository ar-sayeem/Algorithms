#include<stdio.h>
int main()
{
    int T;
    scanf("%d", &T);                // Input test case.

    while(T--)
    {
        int M;
        scanf("%d", &M);            // Input Mark.

        if( M>=80 )
        {
            printf("A+\n");         // 4.00
        }
        else if( M<=80 && M>=75 )
        {
            printf("A\n");          // 3.75
        }
        else if( M<=75 && M>=70 )
        {
            printf("A-\n");         // 3.50
        }
        else if( M<=70 && M>=65 )
        {
            printf("B+\n");         // 3.25
        }
        else if( M<=65 && M>=60 )
        {
            printf("B\n");          // 3.00
        }
        else if( M<=60 && M>=55 )
        {
            printf("B-\n");         // 2.75
        }
        else if( M<=55 && M>=50 )
        {
            printf("C+\n");         // 2.50
        }
        else if( M<=50 && M>=45 )
        {
            printf("C\n");          // 2.25
        }
        else if( M<=45 && M>=40 )
        {
            printf("D\n");          // 2.00
        }
        else
        {
            printf("F\n");          // 0.00
        }
    }
    return 0;
}
// Output Sample:
5
80
A+
75
A
70
A-
65
B+
60
B

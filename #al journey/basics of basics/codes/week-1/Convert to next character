#include<stdio.h>
int main()
{
    char str[100];
    fflush(stdin);      //  fflush(stdin) function is used to clear the input buffer memory of any data that may still be stored.
    gets(str);          // Input character.
    fflush(stdin);
    for(int i=0; str[i]; i++)
        printf("%c", str[i]+1);     // Print next character.
    printf("\n");
    return 0;
}
// Output sample:
dhaka
eiblb
